<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ingenieria.pruebash.BActivity">


    <TextView
        android:id="@+id/textViewNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_marginTop="34dp"
        android:layout_marginStart="18dp"
        android:layout_below="@+id/textViewEdad"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textViewApellidos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_marginTop="34dp"
        android:layout_below="@+id/textViewNombre"
        android:layout_alignStart="@+id/textViewNombre" />

    <TextView
        android:id="@+id/textViewEdad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_marginTop="76dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textViewNombre" />
</RelativeLayout>
